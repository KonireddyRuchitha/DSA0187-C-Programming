#include <iostream>
using namespace std;
int main() {
	int sum=0,n;
	int arr[50],i;
	cout<<"enter size";
	cin>>n;
    cout<<"enter elements";
    for(i=0;i<n;i++)
    {
    cin>>arr[i];
}
  for(i = 0; i < n; i++)
  {
    sum = sum + arr[i];
  }
  cout << "The sum of the elements in the array: " << sum;
}
